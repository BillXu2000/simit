%%% neg
%! neg(42.0) == -42.0;
%! neg(0.0) == -0.0;
func neg(a : float) -> (c : float)
  c = -a;
end

%%% neg-int
%! neg(42) == -42;
func neg(a : int) -> (c : int)
  c = -a;
end

%%% add
%! add(3.0, 2.0) == 5.0;
func add(a : float, b : float) -> (c : float)
  c = a + b;
end

%%% sub
%! sub(3.0, 2.0) == 1.0;
func sub(a : float, b : float) -> (c : float)
  c = a - b;
end
