%%% empty-element
element Empty
end

%%% springs
element Point
	x : tensor[3](float);
	v : tensor[3](float);
end

element Spring
	m  : float;
	l0 : float;
end

extern points : set{Point};
extern springs : set{Spring}(points,points);

%%% field-access
element Point
	x : tensor[3](float);
	v : tensor[3](float);
	m  : float;
end

func f(p : Point) -> (r : float)
	r = p.m;
end
